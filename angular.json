{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@kapsam2/kutup21": {
      "projectType": "library",
      "root": "projects/kapsam2/kutup21",
      "sourceRoot": "projects/kapsam2/kutup21/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/kapsam2/kutup21/tsconfig.lib.json",
            "project": "projects/kapsam2/kutup21/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/kapsam2/kutup21/src/test.ts",
            "tsConfig": "projects/kapsam2/kutup21/tsconfig.spec.json",
            "karmaConfig": "projects/kapsam2/kutup21/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/kapsam2/kutup21/tsconfig.lib.json",
              "projects/kapsam2/kutup21/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@kapsam2/kutup22": {
